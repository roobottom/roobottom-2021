---js
{
  title: "Listening",
  permalink: "/listening/",
  eleventyExcludeFromCollections: true,
  page: {
    navigationId: 'listening'
  }
}
---
{% extends "../layouts/page.njk" %}
{% block title %}
  <h1>{{ title }}</h1>
{% endblock %}
{% block content %}
  <div class="container container--prose prose">
    {# {{ lastfm.weeklytrackchart | dump }} #}
    {% for track in lastfm.weeklytrackchart.track %}
    <p>{{ track | dump }}</p>
    {% endfor %}
  </div>
{% endblock %}