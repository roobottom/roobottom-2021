---js
{
  title: "Articles",
  permalink: "/articles/",
  eleventyExcludeFromCollections: true,
  page: {
    navigationId: 'articles'
  }
}
---
{% extends "../layouts/page.njk" %}
{% block content %}
  <h1>{{ title }}</h1>
  <ol>
  {% for article in collections.articles %}
  <li><a href="{{ article.url }}">{{ article.data.title }}</a></li>
  {% endfor %}
  </ol>
{% endblock %}